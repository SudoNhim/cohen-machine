{
  "name": "poem-machine",
  "version": "1.0.0",
  "description": "A web app for exploring a poet's corpus",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "clean": "rimraf dist",
    "type-check": "tsc -p ./tsconfig.json",
    "build-client": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack -p",
    "dev-client": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack-dev-server -w --hot",
    "build-server": "tsc -p ./src/server",
    "dev-server": "tsc-watch -p ./src/server --onSuccess \"node index.js --debug\"",
    "dev": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev-server\" \"npm run dev-client\"",
    "build": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run build-server\" \"npm run build-client\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SudoNhim/poem-machine.git"
  },
  "engines": {
    "node": "10.x"
  },
  "author": "Brenton Milne & Gabriela Chuchro",
  "license": "ISC",
  "dependencies": {
    "cohen-db": "github:SudoNhim/cohen-db#14f9b59",
    "ejs": "^2.7.4",
    "express": "^4.17.1",
    "jsonpath": "^1.0.2",
    "lunr": "^2.3.8",
    "mongodb": "^3.3.4",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.2",
    "@babel/polyfill": "^7.7.0",
    "@babel/preset-env": "^7.7.1",
    "@babel/preset-react": "^7.7.0",
    "@babel/preset-typescript": "^7.7.2",
    "@types/express": "^4.17.2",
    "@types/jsonpath": "^0.2.0",
    "@types/lunr": "^2.3.2",
    "@types/mongodb": "^3.3.11",
    "@types/node": "^12.12.11",
    "@types/react": "^16.9.11",
    "@types/react-redux": "^7.1.5",
    "@types/react-router": "^5.1.3",
    "@types/webpack": "^4.41.0",
    "@types/webpack-dev-server": "^3.4.0",
    "@types/webpack-manifest-plugin": "^2.1.0",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.2",
    "cross-env": "^5.2.1",
    "css-loader": "2.1.1",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "markdown-react-js": "^1.0.2",
    "postcss-loader": "^3.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-hot-loader": "^4.12.18",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.4",
    "request": "^2.88.0",
    "rimraf": "^2.7.1",
    "style-loader": "^0.23.1",
    "ts-node": "^8.5.2",
    "tsc-watch": "^2.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0",
    "webpack-manifest-plugin": "^2.2.0"
  }
}
