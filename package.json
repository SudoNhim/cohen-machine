{
  "name": "poem-machine",
  "version": "1.0.0",
  "description": "A web app for exploring a poet's corpus",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "start:prod": "cross-env NODE_ENV=production node index.js",
    "clean": "rimraf dist",
    "type-check": "tsc -p ./tsconfig.json",
    "build-client": "cross-env NODE_ENV=production TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack -p",
    "dev-client": "cross-env TS_NODE_PROJECT=\"tsconfig-for-webpack-config.json\" webpack-dev-server -w --hot",
    "build-server": "tsc -p ./src/server",
    "dev-server": "tsc-watch -p ./src/server --onSuccess \"node index.js\"",
    "dev": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run dev-server\" \"npm run dev-client\"",
    "build": "npm run clean && concurrently --prefix \"[{name}]\" --names \"SERVER,CLIENT\" -c \"bgBlue.bold,bgGreen.bold\" \"npm run build-server\" \"npm run build-client\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SudoNhim/poem-machine.git"
  },
  "author": "Brenton Milne & Gabriela Chuchro",
  "license": "ISC",
  "dependencies": {
    "ejs": "^2.6.2",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/express": "^4.17.0",
    "@types/node": "^12.6.8",
    "@types/react": "^16.8.23",
    "@types/react-redux": "^7.1.1",
    "@types/react-router": "^5.0.3",
    "@types/webpack": "^4.32.0",
    "@types/webpack-dev-server": "^3.1.7",
    "@types/webpack-manifest-plugin": "^2.0.0",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "css-loader": "2.1.1",
    "cssnano": "^4.1.10",
    "dotenv": "^8.0.0",
    "postcss-loader": "^3.0.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.12.9",
    "react-redux": "^7.1.0",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "^4.0.4",
    "request": "^2.88.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "ts-node": "^8.3.0",
    "tsc-watch": "^2.2.1",
    "tsconfig-paths": "^3.8.0",
    "tslib": "^1.10.0",
    "typescript": "^3.5.3",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2",
    "webpack-manifest-plugin": "^2.0.4"
  }
}
